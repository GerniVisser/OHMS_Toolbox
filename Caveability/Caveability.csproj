<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
    <ApplicationIcon>Resources\ohms_logo_oBF_icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Resources\OHMS logo.png" />
    <None Remove="Resources\ohms_logo_oBF_icon.ico" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Syncfusion.Licensing" Version="19.3.0.48" />
    <PackageReference Include="Syncfusion.Pdf.Wpf" Version="19.3.0.48" />
    <PackageReference Include="Syncfusion.PropertyGrid.WPF" Version="19.3.0.48" />
    <PackageReference Include="Syncfusion.SfChart.WPF" Version="19.3.0.48" />
    <PackageReference Include="Syncfusion.Shared.WPF" Version="19.3.0.48" />
    <PackageReference Include="Syncfusion.Tools.WPF" Version="19.3.0.48" />
    <PackageReference Include="System.Drawing.Common" Version="6.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Resources\OHMS logo.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Resources\ohms_logo_oBF_icon.ico">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
