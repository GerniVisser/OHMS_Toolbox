<Project Sdk="Microsoft.NET.Sdk">

      <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net5.0-windows</TargetFramework>
        <UseWPF>true</UseWPF>
      </PropertyGroup>

      <ItemGroup>
        <None Remove="Icons\calculator.png" />
        <None Remove="Icons\Calculator_white.png" />
        <None Remove="OHMS Toolbox 1.0.lw.xml" />
        <None Remove="Services\OHMS Toolbox 1.0.lw.xml" />
      </ItemGroup>

      <ItemGroup>
        <PackageReference Include="QLM.QlmLicenseLibNetCore" Version="15.0.21315.2" />
        <PackageReference Include="Syncfusion.Shared.WPF" Version="19.3.0.48" />
        <PackageReference Include="Syncfusion.Tools.WPF" Version="19.3.0.48" />
        <PackageReference Include="Syncfusion.UI.WPF.NET" Version="19.3.0.48" />
      </ItemGroup>

	<ItemGroup>
	  <Resource Include="Icons\calculator.png" />
	  <Resource Include="Icons\Calculator_white.png">
	    <CopyToOutputDirectory>Never</CopyToOutputDirectory>
	  </Resource>
	  <Content Include="OHMS Toolbox 1.0.lw.xml">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	  <Content Include="Services\OHMS Toolbox 1.0.lw.xml">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </Content>
	</ItemGroup>

</Project>
