<Window x:Class="Toolbox.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Toolbox"
        xmlns:views="clr-namespace:Toolbox.Views"
        mc:Ignorable="d"
        Title="OHMS Toolbox" 
        Icon="pack://application:,,,/Icons/ohms_logo_oBF_icon.ico"
        Height="768" 
        Width="1280" 
        ResizeMode="CanResize">

    <Grid>
        <views:ToolboxView DataContext="{Binding ToolboxViewModel}"/>
    </Grid>
</Window>
